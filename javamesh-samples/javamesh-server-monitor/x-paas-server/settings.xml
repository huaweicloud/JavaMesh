<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
    <mirrors>
        <mirror>
            <id>cmc</id>
            <mirrorOf>*,!artifactory-plugin,!clouddragon-release</mirrorOf>
            <name>Artifactory Maven</name>
            <url>http://cmc.centralrepo.rnd.huawei.com/maven/</url>
        </mirror>
    </mirrors>
    <profiles>
        <profile>
            <id>local</id>
            <repositories>
                <repository>
                    <id>clouddragon-release</id>
                    <url>http://dgg.maven.repo.cmc.tools.huawei.com/artifactory/Product-AdapterG-release/</url>
                </repository>
            </repositories>
            <pluginRepositories>
                <pluginRepository>
                    <id>artifactory-plugin</id>
                    <url>http://dg.artifact.huawei.com/artifactory/maven-local-group/</url>
                </pluginRepository>
            </pluginRepositories>
        </profile>
    </profiles>
    <activeProfiles>
        <activeProfile>local</activeProfile>
    </activeProfiles>
</settings>